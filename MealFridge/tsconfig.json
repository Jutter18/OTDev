{
  "compileOnSave": true,
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": true,
    "sourceMap": true,
    "moduleResolution": "Node",
    "module": "commonjs",
    "target": "ES6",
    "lib": [ "es2015.promise", "ES2015", "es2016", "es5", "dom" ],
    "rootDir": "Scripts/",
    "outDir": "wwwroot/js"
  },
  "include": [
    "Scripts/"
  ],
  "exclude": [
    "node_modules",
    "wwwroot"
  ]
}